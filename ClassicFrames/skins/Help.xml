<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Script file="Help.lua"/>
	<Frame parentKey="CfSpinnerOverlay" parent="HelpFrame" frameLevel="3" frameStrata="HIGH">
		<Anchors>
			<Anchor point="TOPLEFT"/>
			<Anchor point="BOTTOMRIGHT"/>
		</Anchors>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="LoadingSpinner" atlas="auctionhouse-ui-loadingspinner" hidden="false">
					<Size x="146" y="146"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Animations>
			<AnimationGroup parentKey="SpinnerAnim" looping="REPEAT">
				<Rotation childKey="LoadingSpinner" order="1" duration="1" degrees="-360"/>
			</AnimationGroup>
		</Animations>
		<Scripts>
			<OnLoad>
				self.SpinnerAnim:Play()
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>