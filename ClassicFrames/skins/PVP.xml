<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Script file="PVP.lua"/>
	<Frame name="PVPWeeklyChestTemplate" virtual="true" useParentLevel="true" mixin="PVPWeeklyChestMixin">
		<Size x="84" y="70"/>
		<Anchors>
			<Anchor point="TOP" y="-48"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="FlairTexture" atlas="pvpqueue-chest-background">
					<Size x="187" y="152"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="ChestTexture">
					<Size x="187" y="152"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" alphaMode="ADD" alpha=".2">
					<Size x="187" y="152"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnShow method="OnShow"/>
			<OnMouseUp method="OnMouseUp"/>
		</Scripts>
	</Frame>
</Ui>