					<OnLoad>
						TextStatusBar_Initialize(self);
						self.textLockable = 1;
						self.cvar = "statusText";
						self.cvarLabel = "STATUS_TEXT_TARGET";
						self.zeroText = "";
					</OnLoad>
					<OnValueChanged>
						CfUnitFrameHealthBar_OnValueChanged(self, value);
						CfTargetHealthCheck(self, value);
					</OnValueChanged>
					<OnSizeChanged>
						CfUnitFrameHealPredictionBars_UpdateSize(self:GetParent());
					</OnSizeChanged>
				</Scripts>
