	<Button name="CfTargetFrame" inherits="CfTargetFrameTemplate" movable="true">
		<Scripts>
			<OnLoad>
				self.noTextPrefix = true;
				self.showLevel = true;
				self.showPVP = true;
				self.showLeader = true;
				self.showThreat = true;
				self.showPortrait = true;
				self.showClassification = true;
				self.showAuraCount = true;
				TargetFrame_OnLoad(self, "target");
				TargetFrame_CreateSpellbar(self, "PLAYER_TARGET_CHANGED");
				TargetFrame_CreateTargetofTarget(self, "targettarget");
				self:RegisterEvent("PLAYER_TARGET_CHANGED");
				self.threatNumericIndicator:SetScript("OnShow", function() TargetFrame_UpdateAuras() end);
				self.threatNumericIndicator:SetScript("OnHide", function() TargetFrame_UpdateAuras() end);
			</OnLoad>
		</Scripts>
	</Button>
	<Button name="CfFocusFrame" movable="true" inherits="CfTargetFrameTemplate" clampedToScreen="true">
		<Scripts>
			<OnLoad>
				self.noTextPrefix = true;
				self.showLevel = true;
				self.showPVP = true;
				self.maxBuffs = 0;
				self.maxDebuffs = 0;
				self.showLeader = true;
				self.showThreat = true;
				self.showPortrait = true;
				self.showClassification = true;
				self.showAuraCount = false;
				TargetFrame_OnLoad(self, "focus");
				TargetFrame_CreateSpellbar(self, "PLAYER_FOCUS_CHANGED");
				TargetFrame_CreateTargetofTarget(self, "focus-target");
				self:RegisterEvent("PLAYER_FOCUS_CHANGED");
			</OnLoad>
		</Scripts>
	</Button>
